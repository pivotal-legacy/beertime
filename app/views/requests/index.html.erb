<ul>
<% @requests.each do |request| %>
  <li>
  <%=request.created_at.strftime("%F")%> <%= request.beer.name %> <%= link_to("View More", request.beer.url, target: '_blank')%>
  <%= link_to 'Delete',
    request,
    method: :delete,
    data: {confirm: 'Are you sure you want to cancel this request?'}
  %>
  </li>
<% end %>
</ul>
